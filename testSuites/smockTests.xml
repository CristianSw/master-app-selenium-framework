<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Framework Suite" parallel="tests">
    <groups>
        <run>
            <include name="smock"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="selenium.framework.test_components.Listeners"/>
    </listeners>

    <test name="Personal Tests">
        <packages>
            <package name="selenium.framework.tests"/>
        </packages>
    </test>
</suite>